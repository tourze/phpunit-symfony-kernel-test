{
  "name": "tourze/phpunit-symfony-kernel-test",
  "description": "PHPUnit测试集合，为Symfony内核调整准备",
  "type": "library",
  "homepage": "https://github.com/tourze/phpunit-symfony-kernel-test",
  "license": "MIT",
  "require": {
    "composer-runtime-api": "^2.0",
    "doctrine/collections": "^2.3",
    "doctrine/data-fixtures": "^2.0",
    "doctrine/dbal": "^4.0",
    "doctrine/doctrine-bundle": "^2.13",
    "doctrine/doctrine-fixtures-bundle": "^4.0",
    "doctrine/orm": "^3.0",
    "doctrine/persistence": "^4.1",
    "symfony/console": "^7.3",
    "symfony/dependency-injection": "^7.3",
    "symfony/doctrine-bridge": "^7.3",
    "symfony/framework-bundle": "^7.4",
    "symfony/security-core": "^7.3",
    "symfony/yaml": "^7.3",
    "tourze/bundle-dependency": "1.*",
    "tourze/doctrine-resolve-target-entity-bundle": "1.*",
    "tourze/enum-extra": "1.0.*",
    "tourze/symfony-testing-framework": "2.0.*",
    "tourze/user-service-contracts": "1.1.*"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^11.5",
    "symfony/http-kernel": "^7.3",
    "tourze/phpunit-base": "1.*"
  },
  "suggest": {
    "tourze/biz-user-bundle": "需要持久化用户实体时安装（createAdminUser/createNormalUser 将自动启用实体模式）"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\PHPUnitSymfonyKernelTest\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\PHPUnitSymfonyKernelTest\\Tests\\": "tests/"
    }
  }
}